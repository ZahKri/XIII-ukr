[DialogueManagerRandom1]
Speakers=((PawnName="MIG1D",Sentences=("","Тривога! Знайдено труп, знайдено труп! Скасовууйте зустріч!")),(PawnName="MIG2D",Sentences=("","Чорт! Проінформуй штаб! Один з наших вбитий. Відміняйте зустріч!")),(PawnName="MIG3D",Sentences=("","Повідом штаб! Скасовуйте зустріч, я щойно знайшов мертвого!")))

[DialogueManager2]
Speakers=((PawnName="MIG3",Sentences=("Жодних новин. Відбій.")))

[DialogueManager5]
Speakers=((PawnName="Mangouste",Sentences=("Кажу вам, номер XIII отримав три кулі у груди!","Приїхала поліція, я не зміг його забрати. Але запевняю вас, він не міг--")),(PawnName="Wax",Sentences=("Що з його тілом?","Досить! Панове, номер XIII ще живий. Ми повинні позбутися його, перш ніж він зіпсує операцію.","Він знає, хто його зрадив. Він захоче помститися...")),(PawnName="Standwell"))

[DialogueManager3]
Speakers=((PawnName="MIG2",Sentences=("Гей, не так голосно! В мене вуха позакладало. ДОБРЕ, ПРИЙНЯТО! Зрозумів, як це відчувається?")))

[DialogueManager4]
Speakers=((PawnName="MIG1b",Sentences=("Ми з Метью усі вихідні займались корпусом корабля. Наступної неділі будемо наносити фарбу.","Чуєш! Глянь-но!","Гелікоптер! Ось там. Повідом штаб!","Ну добре... під твою відповідальність.","Так, ваша величність...")),(PawnName="MIG3",Sentences=("","Ця розмова викликає у мене морську хворобу!","Що?","Та тут в кожного є Гелікоптер. Я не стану про це доповідати. ","Добре. Якщо це дозволить відпочити від тебе хоч на 5 хвилин...")))

[DialogueManager1]
Speakers=((PawnName="hfAmos",Sentences=("Проникніть у Святилище та знайдіть кімнату для зустрічей. Не можу дочекатися, побачити обличчя Номера I!","Охорона напоготові! Лишайся непоміченим до мого сигналу.")),(PawnName="MIG1A",Sentences=("Ні. Шампанське ще не доставлено, і я не можу зв’язатися з містером Віллардом! Хммм... Скажи їм, що мені платять не за це!","Правильно! Передзвони мені, коли будуть новини.")))

