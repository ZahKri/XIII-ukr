[DialogueManager2]
Speakers=((PawnName="Carrington",Sentences=("Стендвелле! Вимагаю негайного звільнення! СТЕНДВЕЛЛЕ !","Агов! Ти, Принеси мої сигари... Он вони, на столі!","Гей! Синку, ти мене чуєш?","Чи може хтось дати мені мої сигари?!")),(PawnName="XIIIa",Sentences=("","Треба розпитати цього чоловіка...")),(PawnName="SPADS1b",Sentences=("Стулися, Керрінгтоне!","Чому мене пасадили за грати?")))

[DialogueManager4]
Speakers=((PawnName="SPADS3",Sentences=("Отже, Керрінгтон... Вже не такий зухвалий, чи не так? Ха! Ха! Ха!")))

[DialogueManager5]
Speakers=((PawnName="Carrington",Sentences=("","","","","Я що марю…?","Боже! Не вірю своїм очам…","Мій кращий агент живий!","Джонс... Прийом? Що? Яка ще амнезія?","Ах! Мої сигари!","За базою є підйомник.","Допоможи!","Сюди!","Стережись!","Вихід. Сюди!")),(PawnName="XIII",Sentences=("Ось ваш портативний передавач.")),(PawnName="CarringtonS",Sentences=("Швидше. Вони повернуться.","Тільки но глянь на цього розумника!","Давай з ним розберемося! Готовий?","Готовий, синку?","Агов, посильний! Підійди-но. Мені потрібно до вбиральні!","Ідеально!","Прикрий мене!","Aaaaх! В мене влучили!")))

