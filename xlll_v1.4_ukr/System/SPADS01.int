[DialogueManager1]
Speakers=((PawnName="SPADS1",Sentences=("Ці новачки думають, що вони, бляха, Рембо !","Де б це не було, він точно зібрав відбиту команду...")),(PawnName="SPADS3a",Sentences=("Де, в біса, Макколл знайшов цих дурнів?")))

